use sugarcane::Command

fn init(sc) {
  sc.info("hello world")

  c = Command::new("setblock", handle_setblock)
  c.add_arg_block_pos("pos")
  sc.add_command(c)
}

fn handle_setblock(sc, world, player) {
  sc.info("SETBLOCK")
}

fn on_block_place(sc, player, pos, kind) {
  sc.info("block x:", pos.x())
}
